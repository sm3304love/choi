#include<stdio.h>
int main()
{
	int num = 0;  int a = 0; int b = 0; int c = 0;

	scanf_s("%d", &a); 
	scanf_s("%d", &b);
	scanf_s("%d", &num);

	switch (num) {
	case 1:
		c = a + b;
		printf("%d+%d =%d", a, b, c);
		break;
	case 2:
		c = a - b;
		printf("%d-%d =%d", a, b, c);
		break;
	case 3:
		c = a * b;
		printf("%d*%d =%d", a, b, c);
		break;
	case 4:
		c = a % b;
		printf("%d % %d =%d", a, b, c);
		break;
	}
	return 0;
}
