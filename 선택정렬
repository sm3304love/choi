#include<stdio.h>

int a[6] = {1,9,45,2,3,8};
int b = 0;
int min = 0;
int c = 0;
int main() 
{
	for (int i = 0; i < 6; i++)
	{
		min = i;
		for (b = i + 1; b < 6; b++)
		{
			if(a[min]>a[b])
			{
				c = a[min];
				a[min] = a[b];
				a[b] = c;
			
			}
				
		}
		
		printf("%d", a[i]);
	}
	return 0;
}
