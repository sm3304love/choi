#include<stdio.h>

int a[6] = { 1,9,45,2,3,8 };
int b = 0;
int c = 0;
int d = 0;
int select(int a[]);

int main()
{

	select(a);
	return 0;
}

int select(int a[])
{
	for (int i = 0; i < 6; i++)
	{
		int temp = i;
		for (b = i + 1; b < 6; b++)
		{
			if (a[b] < a[temp])
			{
				temp = b;
			}
		}

		c = a[i];
		a[i] = a[temp];
		a[temp] = c;

		for (int j = 0; j < 6; j++)
		{
			printf("%d ", a[j]);
		}
		printf("\n");
	}
}
